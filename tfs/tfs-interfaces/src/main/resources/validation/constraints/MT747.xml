<constraint-mappings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:schemaLocation="http://jboss.org/xml/ns/javax/validation/mapping validation-mapping-1.0.xsd"
                     xmlns="http://jboss.org/xml/ns/javax/validation/mapping">
    <default-package>org.ucpb.tfs.interfaces.swift</default-package>
    <bean class="MT747" ignore-annotations="true">
        <class>
            <constraint annotation="com.ucpb.tfs.interfaces.swift.validation.DependentField">
                <element name="dependentField">field32B</element>
                <element name="targetFields">
                    <value>field33B</value>
                    <value>field34B</value>
                </element>
            </constraint>
        </class>
    </bean>

    <constraint-definition annotation="com.ucpb.tfs.interfaces.swift.validation.DependentField">
        <validated-by include-existing-validators="true">
            <value>com.ucpb.tfs.interfaces.swift.validation.DependentFieldValidator</value>
        </validated-by>
    </constraint-definition>
</constraint-mappings>