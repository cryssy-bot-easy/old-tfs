<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:util="http://www.springframework.org/schema/util"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
       http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">

    <import resource="mt103.xml"/>
    <import resource="mt202.xml"/>
    <import resource="mt700.xml"/>
    <import resource="mt740.xml"/>
    <import resource="mt747.xml"/>
    <import resource="mt752.xml"/>
    <import resource="mt760.xml"/>
    <import resource="mt767.xml"/>
    <import resource="mt799.xml"/>


    <bean id="swiftMessageBuilder" class="com.ucpb.tfs.swift.SpelEvaluatingSwiftMessageBuilder">
        <constructor-arg name="propertyMapping">
            <util:map>
                <entry key="basicHeader.applicationIdentifier" value="'F'"/>
                <entry key="basicHeader.serviceIndentifier" value="'01'"/>
                <entry key="basicHeader.ltIdentifier" value="'TLBPPHMMAXXX'"/>
                <entry key="basicHeader.sessionNumber" value="'0000'"/>
                <entry key="basicHeader.sequenceNumber" value="'0000000'"/>

                <entry key="applicationHeader.ioIdentifier" value="'I'"/>
                <!--<entry key="applicationHeader.messageType" value="#this.get('messageType')"/>-->
                <!--<entry key="applicationHeader.receiverAddress" value="'XXXX'"/>-->
                <entry key="applicationHeader.messagePriority" value="'N'"/>
                <entry key="applicationHeader.deliveryMonitoring" value="'000000'"/>
                <entry key="applicationHeader.obsolescencePeriod" value="'000'"/>
            </util:map>
        </constructor-arg>
        <property name="beanMappers">
            <util:map>
                <entry key="103" value-ref="mt103Mapper"/>
                <entry key="202" value-ref="mt202Mapper"/>
                <entry key="700" value-ref="mt700Mapper"/>
                <entry key="740" value-ref="mt740Mapper"/>
                <entry key="747" value-ref="mt747Mapper"/>
                <entry key="752" value-ref="mt752Mapper"/>
                <entry key="760" value-ref="mt760Mapper"/>
                <entry key="767" value-ref="mt767Mapper"/>
                <entry key="799" value-ref="mt799Mapper"/>
            </util:map>
        </property>
    </bean>

</beans>