<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:util="http://www.springframework.org/schema/util"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">

    <bean id="mt799Mapper" class="com.ucpb.tfs.swift.message.builder.SpelEvaluatingMessageBlockBuilder">
        <constructor-arg name="tagMapping">
            <util:map>
                <entry value="20" key="#this.get('documentNumber')"/>
                <entry value="21" key="#this.get('relatedReferences')"/>
                <entry value="79" key="#this.get('reasonForCancellation')"/>
            </util:map>
        </constructor-arg>
    </bean>

</beans>