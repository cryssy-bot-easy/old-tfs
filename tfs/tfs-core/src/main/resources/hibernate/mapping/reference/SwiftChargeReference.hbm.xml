<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-
        //Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-access="field">

    <class name="com.ucpb.tfs.domain.reference.SwiftChargeReference" table="RefSwiftCharge">

        <id column="id" name="id">
            <generator class="org.hibernate.id.IncrementGenerator"/>
        </id>

        <property name="code" column="code" type="java.lang.String" length="10"/>

        <property name="description" column="description" type="java.lang.String" length="255"/>

    </class>

</hibernate-mapping>
