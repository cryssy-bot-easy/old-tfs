<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-
        //Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-access="field">

    <class name="com.ucpb.tfs.domain.mt.OutgoingMT" table="OutgoingMT">

        <id column="id" name="id" type="string" length="36">
            <generator class="org.hibernate.id.UUIDHexGenerator"/>
        </id>

        <property name="destinationBankCode" type="string" length="11"/>

        <property name="messageType" type="string" length="5"/>

        <property name="transactionType" column="status" length="7">
            <type name="org.hibernate.type.EnumType">
                <param name="type">12</param>
                <param name="enumClass">com.ucpb.tfs.domain.mt.TransactionType</param>
            </type>
        </property>

        <property name="MTDetails" column="details" type="org.hibernate.type.MaterializedClobType" length="400000" access="property"/>

    </class>

</hibernate-mapping>
