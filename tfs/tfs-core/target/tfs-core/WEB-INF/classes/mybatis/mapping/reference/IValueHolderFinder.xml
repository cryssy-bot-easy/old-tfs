<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ucpb.tfs.application.query.reference.IValueHolderFinder">

    <select id="getValue" resultType="hashMap">
        select
          vh.token,
          vh.value
          from ValueHolder vh
          where vh.token = #{token}
    </select>


</mapper>