<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-
       //Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-access="field">
    <class name="com.ucpb.tfs.domain.sysparams.RefAdditionalCondition" table="REFADDITIONALCONDITION">
        <id name="id" column="ID" type="java.lang.Long">
            <generator class="increment"/>
        </id>
        <property name="conditionType" column="CONDITIONTYPE" type="string" length="7"/>
        <property name="conditionCode" column="CONDITIONCODE" type="string" length="10"/>
        <property name="condition" column="CONDITION" type="string" length="7000"/>
    </class>
</hibernate-mapping>