<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-
        //Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-access="field">

    <class name="com.ucpb.tfs.domain.cdt.CDTReportControlReference" table="CDTREPORTCONTROLREF">

        <id name="id" column="id" length="36">
            <generator class="org.hibernate.id.UUIDHexGenerator"/>
        </id>

        <property name="unitCode" column="UNITCODE" type="string" length="3"/>

        <property name="firstInitial" column="FIRSTINIT" type="string" length="3"/>

        <property name="shortCode" column="SHORTCODE" type="string" length="3"/>

    </class>

</hibernate-mapping>
