<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-
        //Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-access="field">

    <class name="com.ucpb.tfs.domain.reference.ParameterFileReference" table="DW_REFERENCE">
        <id column="id" name="id">
            <generator class="increment"/>
        </id>

        <property name="field" column="field" type="string" length="20"/>

        <property name="productId" column="productId" type="string" length="20"/>

        <property name="description" column="description" type="string" length="50"/>

        <property name="type" column="ref_type" type="string" length="10"/>

    </class>

</hibernate-mapping>
