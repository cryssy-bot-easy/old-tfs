<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-
        //Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-access="field">

    <class name="com.ucpb.tfs.domain.reference.ValueHolder" table="ValueHolder">

        <id column="id" name="id" length="36">
            <generator class="org.hibernate.id.UUIDHexGenerator"/>
        </id>

        <property name="token" column="token" type="string" length="50"/>
        <property name="value" column="value" type="string" length="50"/>

    </class>

</hibernate-mapping>
