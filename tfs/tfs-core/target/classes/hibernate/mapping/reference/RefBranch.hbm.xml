<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-
       //Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-access="field">

    <class name="com.ucpb.tfs.domain.reference.Branch" table="REF_BRANCH">

        <id name="id" column="id">
            <generator class="increment"/>
        </id>

        <property name="bocCode" column="boc_code" type="string" length="10"/>
        <property name="unitCode" column="unit_code" type="string" length="5"/>
        <property name="casaCreditLimit" column="credit_limit" type="java.math.BigDecimal" precision="31" scale="2"/>
    </class>

</hibernate-mapping>